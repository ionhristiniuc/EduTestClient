@model IEnumerable<TeacherApp.Models.CourseModel>
@{
    ViewBag.Title = "Courses";
}

@Html.ActionLink("Add course", "AddCourse", null, new {@class = "pull-left"})
<br/><br />

<!-- content -->
<div class="col-md-10">
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="text-muted bootstrap-admin-box-title">Students</div>
            </div>
            <div class="bootstrap-admin-panel-content">
                <table class="table table-striped table-bordered" id="example">
                    <thead>
                        <tr>
                            <th>Rendering engine</th>
                            <th>Browser</th>
                            <th>Platform(s)</th>
                            <th>Engine version</th>
                            <th>CSS grade</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var s in Model)
                        {
                            <tr class="odd gradeX">
                                <td>@s.Name</td>                                
                            </tr>
                        }
                                                                                                                                                                                            
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

